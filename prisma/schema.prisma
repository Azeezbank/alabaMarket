generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model user {
  id             String    @id @default(uuid())   @db.Uuid
  email          String?   @unique
  phone          String?   @unique
  password       String?
  otp            String
  expiresAt      DateTime    
  sign_up_verify     Boolean   @default(false)
  name           String?
  googleId       String    @unique
  createdAt      DateTime  @default(now())
}